schema Company {
  name: string,
  created: date in 2020..2024,
  currency: "USD" | "GBP" | "EUR"
}

schema LineItem {
  description: string,
  quantity: int in 1..10,
  unit_price: decimal in 10..500
}

schema Invoice {
  status: 0.6: "paid" | 0.3: "pending" | 0.1: "draft",  // weighted superposition
  issue_date: date in 2023..2024,
  line_items: 1..5 of LineItem
}

dataset TestData {
  companies: 10 of Company,
  invoices: 50 of Invoice
}
